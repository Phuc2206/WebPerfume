{
  "version": "1.0",
  "defaultProvider": "cdnjs",
	"libraries": [
		{
			"library": "twitter-bootstrap@5.2.3",
			"destination": "wwwroot/lib/twitter-bootstrap/"
		},
		{
			"library": "font-awesome@6.2.1",
			"destination": "wwwroot/lib/font-awesome/"
		},
		{
			"library": "jquery@3.6.1",
			"destination": "wwwroot/lib/jquery"
		},
		{
			"library": "jqueryui@1.13.2",
			"destination": "wwwroot/lib/jqueryui"
		},
		{
			"library": "elfinder@2.1.61",
			"destination": "wwwroot/lib/elfinder"
		},
		{
			"provider": "unpkg",
			"library": "@microsoft/signalr@latest",
			"destination": "wwwroot/js/signalr/",
			"files": [
				"dist/browser/signalr.js",
				"dist/browser/signalr.min.js"
			]
		}
	]
}